{
  "name": "discordjs-nakaaa",
  "version": "2.0.0",
  "description": "Simple bot but have some feature built with discord js v14 and typescript",
  "main": "index.ts",
  "scripts": {
    "start": "npm run clean && npm run build && npm run copy && npm run exec",
    "dev": "nodemon --config nodemon.json",
    "prod": "npm run clean && npm run build && npm run copy && npm run package && npm run env && npm run misc && npm run readme",
    "exec": "node ./dist/index.js",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "clean": "rimraf ./dist",
    "copy": "copyfiles -u 1 \"./src/dashboard/views/**/*\" \"./dist/src/\" && copyfiles -u 1 \"./src/dashboard/public/**/*\" \"./dist/src/\"",
    "package": "copyfiles -f \"./package.json\" \"./dist/\" && copyfiles -f \"./package-lock.json\" \"./dist/\"",
    "env": "copyfiles -f \"./.env.example\" \"./dist/\"",
    "misc": "copyfiles -f \"./.gitignore\" \"./dist/\" && copyfiles -f \"./.editorconfig\" \"./dist/\"",
    "readme": "copyfiles -f \"./note.md\" \"./dist/\" && copyfiles -f \"./README.md\" \"./dist/\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepy4k/discordjs-nakaaa-ts.git"
  },
  "keywords": [
    "github",
    "discordjs",
    "discord-bot",
    "discord",
    "discord.js",
    "express",
    "nodejs",
    "node",
    "javascript",
    "api"
  ],
  "author": "benjamin4k",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleepy4k/discordjs-nakaaa-ts/issues"
  },
  "homepage": "https://github.com/sleepy4k/discordjs-nakaaa-ts#readme",
  "dependencies": {
    "@discord-player/extractor": "^4.4.5",
    "@discordjs/opus": "^0.9.0",
    "axios": "^1.3.4",
    "cookie-session": "^2.0.0",
    "cpu-stat": "^2.0.1",
    "debug": "^4.3.4",
    "discord-api-types": "^0.37.66",
    "discord-player": "^6.1.1",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "ffmpeg-static": "~5.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "ms": "2.1.3",
    "node_characterai": "^1.2.1",
    "os": "^0.1.2",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.48",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.0.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "private": true,
  "engines": {
    "node": ">=16.13.0"
  }
}
